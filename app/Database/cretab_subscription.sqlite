CREATE TABLE subscription (
	id integer NOT NULL PRIMARY KEY AUTOINCREMENT , 
	user_id integer NOT NULL , 
	access_id integer NOT NULL , 
	startDate date NOT NULL , 
	endDate date NOT NULL 
);



