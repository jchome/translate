CREATE TABLE param_category (
	id integer NOT NULL PRIMARY KEY AUTOINCREMENT , 
	title varchar(255) NOT NULL , 
	category_id integer NOT NULL , 
	type ENUM("Libre","Liste") NOT NULL 
);



