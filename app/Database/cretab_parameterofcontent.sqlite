CREATE TABLE param_content (
	id integer NOT NULL PRIMARY KEY AUTOINCREMENT , 
	content_id integer NOT NULL , 
	parameter_id integer NOT NULL , 
	free_value varchar(255) , 
	selected_value_id integer 
);



